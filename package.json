{
  "name": "gatsby-ts-mui-starter",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby fully integrated with typescript, material ui and story book.",
  "version": "0.1.0",
  "author": "Hung Tsou <hungtsougg@gmail.com>",
  "dependencies": {
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-helmet": "^5.0.8",
    "clsx": "^1.0.4",
    "gatsby": "^2.13.44",
    "gatsby-image": "^2.2.7",
    "gatsby-plugin-layout": "^1.1.2",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-material-ui": "^2.1.4",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-plugin-typescript": "^2.1.2",
    "gatsby-source-filesystem": "^2.1.6",
    "gatsby-transformer-sharp": "^2.2.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "storybook-addon-material-ui": "^0.9.0-alpha.20"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-info": "^5.1.10",
    "@types/node": "^12.6.9",
    "@types/storybook__addon-knobs": "^5.0.3",
    "@types/storybook__react": "^4.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-remove-graphql-queries": "^2.7.2",
    "husky": "^3.0.3",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.1.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3"
  },
  "keywords": [
    "gatsby",
    "react",
    "PWA",
    "typescript",
    "material ui",
    "story book"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -p 8001",
    "format": "prettier --write src/**/*.{ts,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "lint:ts": "tslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "storybook": "NODE_ENV=production start-storybook -s ./public -p 6006",
    "build-storybook": "NODE_ENV=production build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint:ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
